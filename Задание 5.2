N, M = map(int, input().split())

dist = [[10**9] * N for _ in range(N)]

for i in range(M):
    u, v, w = map(int, input().split())
    u -= 1
    v -= 1
    dist[u][v] = w
    dist[v][u] = w

for k in range(N):
    for i in range(N):
        for j in range(N):
            dist[i][j] = min(dist[i][j], dist[i][k] + dist[k][j])

best = 1
min_max = 10**9

for i in range(N):
    curr_max = max(dist[i])
    if curr_max < min_max:
        min_max = curr_max
        best = i + 1

print(f"Результат: {best}")
